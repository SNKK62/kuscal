var a: Array<f64>[5] = [1, 2, 3, 4];
var b: f64 = 10;
var i: f64 = 0;

while i < 5 {
    print(a[i]);
    b = a[i];
    a[i] = pow(b, 2);
    i = i + 1;
}

i = 0;

while i < 5 {
    print(a[i]);
    i = i + 1;
}

for j in 0 to 5 {
    if j < 2 {
        continue;
    } else {
        if j > 2 {
            break;
        };
        print(a[j]);
    }
}
